{
    "name": "alwaysseptember/open-docs",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "danack/aurynconfig": "^0.9.1",
        "danack/configurator": "1.1.1",
        "danack/console": "^2.6",
        "danack/datatype": "^0.5.1",
        "danack/esprintf": "2.1.0",
        "danack/float-hex": "^1.0",
        "danack/looping-exec": "^0.3.0",
        "danack/json-safe": "^0.1.1",
        "danack/rfc-codex": "dev-master",
        "danack/slim4-auryn-invoker": "0.1.1",
        "danack/var-map": "^0.6.0",
        "justinrainbow/json-schema": "^5.2.7",
        "laminas/laminas-diactoros":"^2.24.0",
        "league/commonmark": "^2.0.2",
        "michelf/php-markdown": "^1.9",
        "ramsey/uuid": "4.0.1",
        "rdlowrey/auryn": "^1.4.2",
        "seld/jsonlint": "^1.7",
        "slim/slim": "4.11.0",
        "thecodingmachine/safe": "^1.3",
        "tholu/php-cidr-match": "^0.2.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-posix": "*",
        "ext-redis": "*"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink-extension": "^2.3",
        "danack/coding-standard": "0.3.0",
        "dmore/behat-chrome-extension": "^1.1",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "fluentdom/fluentdom": "^8.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "9.5.6",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "slevomat/coding-standard": "^7.0.13",
        "squizlabs/php_codesniffer": "^3.7.1",
        "yoast/yoastcs": "2.1.0"
    },
    "autoload": {
        "psr-0": {
            "Internals\\": "src/",
            "Learning\\": "src/",
            "Merch\\": "src/",
            "NamingThings\\": "src/",
            "OpenDocs\\": "src/",
            "PHPFunding\\": "src/",
            "PhpOpenDocs\\": "src/",
            "RfcCodexOpenDocs\\": "src/",
            "System\\": "src/",
            "Work\\": "src/"
        },
        "files": [
            "src/Internals/functions.php",
            "src/Learning/functions.php",
            "src/NamingThings/functions.php",
            "src/Work/functions.php",
            "src/PhpOpenDocs/functions.php",
            "src/PHPFunding/functions.php",
            "src/RfcCodexOpenDocs/functions.php",
            "src/System/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-0": {
            "PhpOpenDocsTest\\": "test/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
